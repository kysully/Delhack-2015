<!doctype html>
<html ng-app="live-map-app">
  <head>
    <title>{{deal.r_name}}</title>
    <link rel="icon" type="image/png" href="/img/heat_logo.png">

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
    <link rel="stylesheet" href="http://cdn.leafletjs.com/leaflet/v0.7.7/leaflet.css">
    <link rel="stylesheet" href="/css/live_style.css">
  </head>
  <body ng-controller="DealController">
    <% include ./partials/nav %>
    <div class="container">
      <h1>{{deal.r_name}}</h1>
      <h4>{{deal.f_name}} - {{deal.code}}</h4>
      <p>
        {{deal.description}}
      </p>
    </div>

    <% include ./partials/vendor %>
    <script type="text/javascript" src="/js/live_map.js"></script>
  </body>
</html>
